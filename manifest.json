{
  "manifest_version": 3,
  "name": "ChatGPT Cmd+Enter",
  "version": "1.0.0",
  "description": "Send messages in ChatGPT using Cmd+Enter instead of Enter",
  "permissions": ["activeTab"],
  "icons": {
    "128": "icon/icon_128.png",
    "48": "icon/icon_48.png",
    "16": "icon/icon_16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://chat.openai.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
